<Window x:Class="RegexTool.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RegexTool.UI"
        mc:Ignorable="d"
        xmlns:v="clr-namespace:RegexTool.UI.View"
        xmlns:vm="clr-namespace:RegexTool.UI.ViewModel"
        WindowState="Maximized"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <DataTemplate DataType="{x:Type vm:ShellViewModel}">
            <v:ShellView/>
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <ContentControl>
            <vm:ShellViewModel/>
        </ContentControl>
    </Grid>
</Window>
